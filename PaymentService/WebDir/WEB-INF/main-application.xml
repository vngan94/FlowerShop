<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "https://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean class="com.tttn.api.ApplicationService" init-method="startup"
		  destroy-method="shutdown" name="AppService">
			<property name="handlers">
				<map>
					<entry key="v1/GetDemo">
						<bean class="com.tttn.api.handlers.GetDemoHandler" />
					</entry>
					<entry key="v1/GetAllRole">
						<bean class="com.tttn.api.handlers.read.GetAllRoleHandler" />
					</entry>
					<entry key="v1/PostAccount">
						<bean class="com.tttn.api.handlers.write.PostAccountHandler" />
					</entry>
					<entry key="v1/GetGiftVoucherByKey">
						<bean class="com.tttn.api.handlers.read.GetGiftVoucherByKeyHandler" />
					</entry>
					<entry key="v1/PostGiftVoucher">
						<bean class="com.tttn.api.handlers.write.PostGiftVoucherHandler" />
					</entry>

					<entry key="v1/UpdateGiftVoucher">
						<bean class="com.tttn.api.handlers.write.UpdateGiftVoucherHandler" />
					</entry>
					<entry key="v1/GetPaymentTransactionByIdTrans">
						<bean class="com.tttn.api.handlers.read.GetPaymentTransactionByIdTransHandler" />
					</entry>
					<entry key="v1/PostPaymentTransaction">
						<bean class="com.tttn.api.handlers.write.PostPaymentTransactionHandler" />
					</entry>



				</map>
			</property>
	</bean>
</beans>